{
  "scheming_version": 1,
  "dataset_type": "dataset",
  "about_url": "https://github.com/depositar-io/ckanext-data-depositario",
  "dataset_fields": [
    {
      "field_name": "title",
      "label": {
        "en": "Title",
	"zh_TW": "標題"
      },
      "preset": "title",
      "form_placeholder": {
        "en": "eg. A descriptive title",
	"zh_TW": "例如：一個描述性的標題"
      }
    },
    {
      "field_name": "name",
      "label": {
        "en": "URL",
	"zh_TW": "網址"
      },
      "preset": "dataset_slug",
      "validators": "not_empty unicode package_name_validator",
